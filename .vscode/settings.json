	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"cSpell.words": [
		"browserslist",
		"figcaption",
		"geocities",
		"Goodreads",
		"gtag",
		"Inspirato",
		"keyof",
		"Nextra",
		"pageview",
		"Stackoverflow",
		"stylelint"
	],
	"prettier.eslintIntegration": true,
	"prettier.stylelintIntegration": true
}
